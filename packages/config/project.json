{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "packages/config/src",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "outputs": ["{projectRoot}/build"],
            "options": {
                "cwd": "{projectRoot}",
                "command": "cpx \"src/**/!(*.ts)\" build/"
            }
        }
    }
}
